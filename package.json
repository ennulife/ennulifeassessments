{
  "name": "ennulife-assessments",
  "version": "1.0.0",
  "description": "A comprehensive assessment plugin for Ennu Life.",
  "main": "ennu-life-plugin.php",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "wp-scripts build assets/js/ennu-admin.js assets/js/user-dashboard.js assets/js/ennu-frontend-forms.js --output-path=dist",
    "env:start": "wp-env start",
    "env:stop": "wp-env stop",
    "env:run": "wp-env run",
    "test:php": "npx wp-env run tests-wordpress vendor/bin/phpunit",
    "test:php:coverage": "npx wp-env run tests-wordpress vendor/bin/phpunit --coverage-html coverage-php"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "assessment"
  ],
  "author": "Ennu Life",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@wordpress/env": "^8.0.0",
    "@wordpress/scripts": "^30.0.0",
    "cypress": "^13.0.0"
  }
}
